{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest/globals": true
  },
  "rules":{
    "linebreak-style": 0,
    "global-require": 0,
    "no-console": 0,
    "no-template-curly-in-string": 0,
    "import/no-dynamic-require": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  },
  "extends": [
    "airbnb"
  ],
  "plugins": [
    "import"
  ]
}
